[run]
omit =
    *tests.py
    *__init__.py
branch = True
source =
    copy_code/commands

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError
    if __name__ == .__main__.:
    from urllib.parse import parse_qs
    except ImportError:
